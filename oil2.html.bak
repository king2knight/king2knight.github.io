<div class="container mt-5">
    <h1>Video Player with Bookmarks</h1>
    <div class="row justify-content-center">
        <div>
            <video id="videoPlayer" controls>
                <source src="images/Oil and Gold.mp4" type="video/mp4">
                Your browser does not support the video tag.
            </video>
        </div>

    </div>
</div>
	<div class="row justify-content-center">
	        <div>
            <h4 class="text-white" style="padding-top:100px;padding-bottom:-10px;text-align:center;background-color:black;">Bookmarks</h4>
            <button class="btn btn-primary" onclick="jumpToBookmark(10)">Bookmark at 0:10</button>
            <button class="btn btn-primary" onclick="jumpToBookmark(15)">Bookmark at 0:15</button>
        </div>
	</div>
<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.3/dist/umd/popper.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
<script>
    // Get references to the video element and bookmarks
    const video = document.getElementById("videoPlayer");
    
    // Function to jump to a specific time in the video (in seconds)
    function jumpToBookmark(timeInSeconds) {
        video.currentTime = timeInSeconds;
    }
</script>

